{
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "clean": "rimraf --glob dist *.vsix",
    "dev": "webpack-dev-server --progress   --https --port 9085 --mode development",
    "package:dev": "node ./scripts/packageDev",
    "publish:dev": "npm run package:dev && node ./scripts/publishDev",
    "build:release": "npm run clean && mkdir dist && webpack --progress --output-path ./dist --mode production",
    "publish:release": "npm run build:release && node ./scripts/publishRelease",
    "dev:test": "karma start",
    "test": "karma start --single-run",
    "postinstall": "npm run test"
  },
